#!/bin/sh

conf() {
  printf "
  $(c 0 2)🍣 $(c 0 0)$(c 1 36)$(Name)

  $(c 1 34)🞛 $(c 0 0)$(c 1 34) OS   ⦁ $(Os) ($(Kernel))
  $(c 1 32)🞛 $(c 0 0)$(c 1 32) UP   ⦁ $(Uptime)
  $(c 1 33)🞛 $(c 0 0)$(c 1 33) SH   ⦁ fsh
  $(c 1 35)🞛 $(c 0 0)$(c 1 35) WM   ⦁ $(Desktop)
  $(c 1 31)☰ $(c 0 0)$(c 1 31) MEM  ⦁ $(Memory)
  \n"
  # $(c 1 30)  $(c 1 31)  $(c 1 32)  $(c 1 33)  $(c 1 34)  $(c 1 35)  $(c 1 36)  $(c 1 37)  $(c 0 0)
}
